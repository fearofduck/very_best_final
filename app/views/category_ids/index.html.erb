<div class="page-header">
  <h1>
    Category ids
    <a href="/category_ids/new" class="btn btn-lg btn-success">New Category</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Cuisine</th>
        <th>Actions</th>
      </tr>

      <% @category_ids.each do |category_id| %>
      <tr>
        <td><%= category_id.name %></td>
        <td><%= category_id.cuisine %></td>
        <td>
          <a href="/category_ids/<%= category_id.id %>" class="btn btn-primary">Show</a>
          <a href="/category_ids/<%= category_id.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_category_id/<%= category_id.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
